namespace AlexanderC\Bundle\UngaBungaBundle\Entity;

use FOS\UserBundle\Model\User as BaseUser;

class SystemUser extends BaseUser
{
    /**
     * @var integer
     */
    protected $id;

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    public function __construct()
    {
        parent::__construct();
        // your own logic
    }
}
